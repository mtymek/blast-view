{
    "name": "mtymek/blast-view",
    "description": "Template renderer using templating engine from Zend Framework 2 - Zend\\View.",
    "keywords": [
        "zend view",
        "template",
        "renderer",
        "view helper",
        "layout"
    ],
    "require": {
        "zendframework/zend-servicemanager": "~2.5",
        "zendframework/zend-view": "~2.5",
        "zendframework/zend-escaper": "~2.5",
        "zendframework/zend-filter": "~2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7"
    },
    "license": "2-Clause BSD",
    "authors": [
        {
            "name": "Mateusz Tymek",
            "email": "mtymek@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Blast\\View\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BlastTest\\View\\": "test"
        }
    }
}
